{
  "version": "1.0.1",    
  "name": "CrossDomainVF",     
  "manifest_version": 2 , 
  "description": "CrossDomainVF",   
  "icons": {
	    "32" : "favicon-32x32.png"
	},

  "permissions": [
		"tabs",
		"webRequest", 
		"webRequestBlocking",
        "http://*/*",
		"https://*/*"
	],
  "background": {
    "scripts": [ "background.js"]
  },


 

  "browser_action": {
    "default_icon": "favicon-32x32.png",     
    "default_popup": "popup.html"
  }


}


 
 